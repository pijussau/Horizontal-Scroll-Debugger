{
    "name": "Horizontal Scroll Debugger",
    "version": "1.1",
    "manifest_version" : 2,
    "description": "Quickly find out exactly what is causing the 'swimming'!",
    "background" : {
      "scripts" : ["script.js"],
      "persistent": false
    },
    "browser_action": {
        
    },
    "web_accessible_resources": ["script.js", "popup.html"],
    "permissions": [
        "activeTab"
    ],
    "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png" 
    }
    
  }